{
  "name": "claraleigh/naughty-username",
  "description": "Validate that a username is not a naughty one",
  "type": "library",
  "keywords": [ "laravel", "validation" ],
  "require": {
    "php": "8.*",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.2.*|^8.5.8|^9.3.3|^10.0.0"
  },
  "autoload": {
    "psr-4": {
      "ClaraLeigh\\NaughtyUsername\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NaughtyUsername\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Clara Leigh",
      "email": "nobody@noone.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}